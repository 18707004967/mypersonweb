/* pages/inside/edit_end.wxss */
.edit_top{
	width: 100vw;
	height: 176rpx;
	background: #fff;
	position: absolute;
	left: 0;
	top: 0;
}
.edit_bottom{
	min-height: 100vh;
	width: 100vw;
	overflow: hidden;
}
.edit_change_box{
	width: 100vw;
	height: 850rpx;
	position: relative;
	margin: 200rpx auto 0;
}
.edit_bg,.edit_jg{
	display: block;
	width: 100%;
	height: 100%;
}
.edit_jg{
	position: absolute;
	left: 0;
	top: 0;
}
.edit_body{
	z-index: 5;
}
.edit_head{
	z-index: 10;
}
.edit_leg{
	z-index: 15;
}
.edit_shoe{
	z-index: 20;
}
.edit_bz{
	display: block;
	width: 65rpx;
	height:261rpx;
	position: absolute;
	right: 32rpx;
	top: 44rpx;
}
.edit_write{
	width: 372rpx;
	height: 88rpx;
	border-bottom: #231718 solid 2rpx;
	margin: 44rpx auto 0;
	display: flex;
}
.edit_write_icon{
	display: block;
	width: 40rpx;
	height: 40rpx;
	background: url(data:image/png;base64,) no-repeat;
	background-size: 40rpx 40rpx;
	margin: 22rpx 0 0 4rpx;
}
.edit_ipt{
	display: block;
	width: 276rpx;
	height: 65rpx;
	margin: 6rpx 0 0 10rpx;
	text-align: center;
	font-size: 26rpx;
	color: #020202; 
	line-height: 65rpx;
}
.edit_btn_box{
	width: 100vw;
	display: flex;
	position: absolute;
	left: 0;
	bottom: 30rpx;
}
.edit_btn1{
	width: 112rpx;
	margin: 0 0 0 50rpx;
}
.edit_btn1 button image{
	display: block;
	width: 100%;
	height: 100%;
}
.edit_btn1 button{
	display: block;
	width: 112rpx;
	height: 112rpx;
	border-radius: 50%;
}
.edit_btn_text{
	display: block;
	width: 112rpx;
	font-size: 20rpx;
	color: #040600;
	line-height: 24rpx;
	text-align: center;
	margin: 4rpx auto 0;
}
.edit_btn2{
	margin: 0 0 0 158rpx;
}
.edit_btn3{
	margin: 0 0 0 24rpx;
}
.edit_btn4{
	margin: 0 0 0 446rpx;
}
